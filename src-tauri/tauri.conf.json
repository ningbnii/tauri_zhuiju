{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "zhuiju",
    "version": "1.0.4"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": true,
        "open": true,
        "sidecar": true,
        "execute": true,
        "scope": [
          {
            "name": "binaries/service",
            "sidecar": true
          }
        ]
      }
    },
    "windows": [
      {
        "title": "追剧",
        "width": 1024,
        "height": 768,
        "fullscreen": false,
        "decorations": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.wxbuluo.dev",
      "externalBin": ["binaries/service"],
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
    }
  }
}
